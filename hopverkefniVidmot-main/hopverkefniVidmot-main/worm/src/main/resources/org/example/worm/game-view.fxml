<?xml version="1.0" encoding="UTF-8"?>
<!-- designs the game view, it has a Leikbord and Stats pane in an AnchorPane -->
<?import javafx.scene.layout.AnchorPane?>
<?import org.example.worm.Leikbord?>
<?import org.example.worm.Stats?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.worm.GameController"
            prefHeight="400.0" prefWidth="600.0">

    <Leikbord fx:id="leikbord"/>
    <Stats fx:id="stats"/>
</AnchorPane>
